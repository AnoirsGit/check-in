<div class="page-header">
  <%= link_to working_times_path, class: 'btn btn-default' do %>
    All Working_times
  <% end %>
  <%= link_to edit_working_time_path(@working_time), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show working_time</h1>
</div>

<dl class="dl-horizontal">
  <dt>Started at:</dt>
  <dd><%= print_time @working_time.started_at %></dd>

  <dt>Ended at:</dt>
  <dd><%= print_time @working_time.ended_at %></dd>

  <dt>Bio:</dt>
  <dd><%= @working_time.bio %></dd>

  <dt>User:</dt>
  <dd><%= @working_time.user.email %></dd>

</dl>
