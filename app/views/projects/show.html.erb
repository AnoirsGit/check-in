<div class="page-header">
  <%= link_to projects_path, class: 'btn btn-default' do %>
    All Posts
  <% end %>
  <% if current_user.admin? %>
    <%= link_to edit_project_path(@project), class: 'btn btn-primary' do %>
      Edit
    <% end %>
  <% end %>
  <h1>Show project</h1>
</div>

<dl class="dl-horizontal">

  <dt>Title:</dt>
  <dd><%= @project.title %></dd>
  <dt>description:</dt>
  <dd><%= @project.description %></dd>

</dl>
