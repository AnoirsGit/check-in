<div class="page-header">
  <%= link_to categories_path, class: 'btn btn-default' do %>
    All Categories
  <% end %>
  <%= link_to edit_category_path(@category), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show category</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @category.title %></dd>

  <% @working_times.each do |wt| %>
    <p><%= wt.user.email %> <%= print_time wt.started_at %> <%= print_time wt.ended_at %></p>
  <% end %>

</dl>
